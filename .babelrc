{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
       "browsers": ["> 1%", "last 2 versions", "not ie <=8"]
      },
      "useBuiltIns": "entry",
      "corejs": 2
    }
   ],
   "@babel/preset-react"
  ],
  "plugins": [
    "dynamic-import-webpack",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-syntax-jsx",
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]
  ]
}
